    "Ans:-\n",
    "no=int(input(\"Enter Number Decimal to convert:\"))\n",
    "print(f\"Binary value of {no} is=\",bin(no))\n",
    "print(f\"Octal value of {no} is=\",oct(no))\n",
    "print(f\"Hexadecimal value of {no} is=\",hex(no))\n"
