    "        while(cno>=1):\n",
    "            digi=cno%10\n",
    "            d.append(digi)\n",
    "            cno=int(cno/10)\n",
    "        d.reverse()\n",
    "        for i in range(0,len(d)):\n",
    "            dn=dn+d[i]**2\n",
    "        return dn\n",
    "    except Exception as e:\n",
